local function _tfb()
    _fvv = not _fvv
    for _, _ch in pairs(_frm:GetChildren()) do
        if _ch:IsA("ImageButton") then
            local _fv = _ch:FindFirstChild("favorite")
            if _fv then _fv.Visible = _fvv end
        end
    end
    local _hfb = _frm:FindFirstChild("HideFavoritesBtn")
    if _hfb then _hfb.Text = _fvv and "Hide Favorites" or "Show Favorites" end
    _wff("FavoritesVisible.txt", _0xI0:JSONEncode({visible = _fvv}))
end
_oca = function(_ch)
    for _i, _v in pairs(_frm:GetChildren()) do if not _v:IsA("UIGridLayout") then _v:Destroy() end end
    local _hm = _wfc(_ch, "Humanoid")
    local _ds = _hm:WaitForChild("HumanoidDescription", 5) or Instance.new("HumanoidDescription", _hm)
    local _rt = Instance.new("UIAspectRatioConstraint")
    _rt.AspectType = Enum.AspectType.ScaleWithParentSize
    local _hfb = Instance.new("TextButton")
    _hfb.Name = "HideFavoritesBtn"
    _rt:Clone().Parent = _hfb
    _hfb.LayoutOrder = 0
    _hfb.TextColor3 = Color3.new(1, 1, 1)
    _hfb.BorderSizePixel = 0
    _hfb.BackgroundTransparency = 0.3
    _hfb.BackgroundColor3 = Color3.new(0, 0, 0)
    _hfb.TextScaled = true
    _hfb.Text = _fvv and "Hide Favorites" or "Show Favorites"
    _hfb.Font = Enum.Font.GothamBold
    _hfb:SetAttribute("name", "")
    Instance.new("UICorner").Parent = _hfb
    local _hfs = Instance.new("UIStroke")
    _hfs.Color = Color3.fromRGB(80, 80, 80)
    _hfs.Thickness = 1
    _hfs.Transparency = 0.5
    _hfs.Parent = _hfb
    _hfb.MouseButton1Click:Connect(_tfb)
    _hfb.MouseEnter:Connect(function() _enm.Text = "Toggle Favorites" end)
    _hfb.Parent = _frm
    local function _ceb(_em, _dc, _rc)
        _dc:AddEmote(_em.name, _em.id)
        local _eb = Instance.new("ImageButton")
        local _if = table.find(_fve, _em.id)
        if _ceo[tostring(_em.id)] then _eb.LayoutOrder = _ceo[tostring(_em.id)]
        else local _sv = (_em.sort and _em.sort[_cst]) or _em.index or 0 _eb.LayoutOrder = _sv + ((_if and 0) or #_emt) end
        _eb.Name = tostring(_em.id)
        _eb:SetAttribute("name", _em.name)
        Instance.new("UICorner").Parent = _eb
        _eb.Image = _em.icon
        _eb.BackgroundTransparency = 0.5
        _eb.BackgroundColor3 = Color3.new(0, 0, 0)
        _eb.BorderSizePixel = 0
        _rc:Clone().Parent = _eb
        local _bs = Instance.new("UIStroke")
        _bs.Color = Color3.fromRGB(80, 80, 80)
        _bs.Thickness = 1
        _bs.Transparency = 0.5
        _bs.Parent = _eb
        local _en = Instance.new("TextLabel")
        _en.Name = "number"
        _en.TextScaled = true
        _en.BackgroundTransparency = 1
        _en.TextColor3 = Color3.new(1, 1, 1)
        _en.BorderSizePixel = 0
        _en.AnchorPoint = Vector2.new(0.5, 0.5)
        _en.Size = UDim2.new(0.2, 0, 0.2, 0)
        _en.Position = UDim2.new(0.1, 0, 0.9, 0)
        _en.Text = tostring((_em.sort and _em.sort[_cst]) or _em.index or "")
        _en.TextXAlignment = Enum.TextXAlignment.Center
        _en.TextYAlignment = Enum.TextYAlignment.Center
        _en.Font = Enum.Font.GothamBold
        local _ns = Instance.new("UIStroke")
        _ns.Transparency = 0.5
        _ns.Parent = _en
        _en.Parent = _eb
        _eb.Parent = _frm
        _eb.MouseButton1Click:Connect(function()
            if not _dge then _ple(_em.name, _em.id)
            else
                local _do = _dge.button.LayoutOrder
                local _to = _eb.LayoutOrder
                _dge.button.LayoutOrder = _to
                _eb.LayoutOrder = _do
                _ceo[tostring(_dge.id)] = _to
                _ceo[tostring(_em.id)] = _do
                _wff("CustomEmoteOrder.txt", _0xI0:JSONEncode(_ceo))
                _dge.button.BackgroundColor3 = Color3.new(0, 0, 0)
                _dge.button.BackgroundTransparency = 0.5
                _dge = nil
            end
        end)
        _eb.MouseButton2Down:Connect(function()
            _dge = {button = _eb, id = _em.id, originalOrder = _eb.LayoutOrder}
            _eb.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
            _eb.BackgroundTransparency = 0.2
        end)
        _eb.MouseEnter:Connect(function() _enm.Text = _em.name end)
        local _fv = Instance.new("ImageButton")
        _fv.Name = "favorite"
        _fv.Image = table.find(_fve, _em.id) and _fon or _fof
        _fv.AnchorPoint = Vector2.new(0.5, 0.5)
        _fv.Size = UDim2.new(0.1, 0, 0.1, 0)
        _fv.Position = UDim2.new(0.9, 0, 0.9, 0)
        _fv.BorderSizePixel = 0
        _fv.BackgroundTransparency = 1
        _fv.Visible = _fvv
        _fv.Parent = _eb
        _fv.MouseButton1Click:Connect(function()
            local _ix = table.find(_fve, _em.id)
            if _ix then
                table.remove(_fve, _ix)
                _fv.Image = _fof
                _eb.LayoutOrder = _em.sort[_cst] + #_emt
            else
                table.insert(_fve, _em.id)
                _fv.Image = _fon
                _eb.LayoutOrder = _em.sort[_cst]
            end
            _wff("FavoritedEmotes.txt", _0xI0:JSONEncode(_fve))
        end)
    end
    for _i, _em in pairs(_emt) do _ceb(_em, _ds, _rt) end
    for _i = 1, 9 do
        local _f = Instance.new("Frame")
        _f.LayoutOrder = 2147483647
        _f.Name = "filler"
        _f.BackgroundTransparency = 1
        _rt:Clone().Parent = _f
        _f.Parent = _frm
    end
    _aai = nil
    _aat = nil
    if _aab then
        _aab.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        if _aab:FindFirstChild("TextLabel") then _aab.TextLabel.TextColor3 = Color3.fromRGB(200, 200, 200) end
        _aab = nil
    end
end
