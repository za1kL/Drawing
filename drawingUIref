local function _chkA()
    local _s, _r = pcall(function()
        return game:HttpGet("https://raw.githubusercontent.com/za1kL/made-by-za1k/refs/heads/main/emoteui1")
    end)
    if _s and _r then
        local _t = _r:gsub("%s+", "")
        if _t == "--work" then return true
        elseif _t == "--dont" then _0xJ1.LocalPlayer:Kick("Script is disabled by za1k") return false end
    end
    _0xJ1.LocalPlayer:Kick("Script is disabled by za1k")
    return false
end
if not _chkA() then return end
