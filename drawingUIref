local function _rfl()
    for _, _ch in pairs(_rls:GetChildren()) do
        if _ch:IsA("Frame") then _ch:Destroy() end
    end
    for _i, _rc in pairs(_svr) do
        local _if = Instance.new("Frame")
        _if.Size = UDim2.new(0.95, 0, 0, 30)
        _if.BackgroundTransparency = 0.5
        _if.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        _if.BorderSizePixel = 0
        Instance.new("UICorner").Parent = _if
        local _pb = Instance.new("TextButton")
        _pb.Size = UDim2.new(0.8, 0, 1, 0)
        _pb.Position = UDim2.new(0, 0, 0, 0)
        _pb.BackgroundTransparency = 1
        _pb.Text = "  " .. _rc.name
        _pb.TextColor3 = Color3.new(1, 1, 1)
        _pb.TextScaled = true
        _pb.Font = Enum.Font.Gotham
        _pb.TextXAlignment = Enum.TextXAlignment.Left
        _pb.Parent = _if
        local _nb = Instance.new("TextBox")
        _nb.Size = UDim2.new(0.8, 0, 1, 0)
        _nb.Position = UDim2.new(0, 0, 0, 0)
        _nb.BackgroundTransparency = 1
        _nb.Text = _rc.name
        _nb.TextColor3 = Color3.new(1, 1, 1)
        _nb.TextScaled = true
        _nb.Font = Enum.Font.Gotham
        _nb.TextXAlignment = Enum.TextXAlignment.Left
        _nb.Visible = false
        _nb.Parent = _if
        local _db = Instance.new("TextButton")
        _db.Size = UDim2.new(0.15, 0, 0.8, 0)
        _db.AnchorPoint = Vector2.new(1, 0.5)
        _db.Position = UDim2.new(0.98, 0, 0.5, 0)
        _db.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        _db.BackgroundTransparency = 0.3
        _db.Text = "X"
        _db.TextColor3 = Color3.new(1, 1, 1)
        _db.TextScaled = true
        _db.Font = Enum.Font.GothamBold
        Instance.new("UICorner").Parent = _db
        _db.Parent = _if
        _if.Parent = _rls
        _db.MouseButton1Click:Connect(function()
            table.remove(_svr, _i)
            _wff("EmoteRecordings.txt", _0xI0:JSONEncode(_svr))
            _rfl()
        end)
        _pb.MouseButton2Up:Connect(function()
            _pb.Visible = false
            _nb.Visible = true
            _nb:CaptureFocus()
        end)
        _nb.FocusLost:Connect(function()
            _pb.Visible = true
            _nb.Visible = false
            if _nb.Text ~= "" then
                _svr[_i].name = _nb.Text
                _pb.Text = "  " .. _nb.Text
                _wff("EmoteRecordings.txt", _0xI0:JSONEncode(_svr))
            end
        end)
        _pb.MouseButton1Click:Connect(function()
            task.spawn(function()
                if _isr then return end
                _snd("Playback", "Playing " .. _rc.name .. "...")
                local _st = tick()
                for _, _ev in ipairs(_rc.data) do
                    local _tt = _ev.time
                    local _ce = tick() - _st
                    local _wd = _tt - _ce
                    if _wd > 0 then task.wait(_wd) end
                    local _ch = _0xJ1.LocalPlayer.Character
                    local _hm = _ch and _ch:FindFirstChildOfClass("Humanoid")
                    if not _hm then continue end
                    if _ev.id == "STOP" then
                        if _aat then _aat:Stop() _aat:Destroy() _aat = nil end
                        _aai = nil
                        if _aab then
                            _0xH9:Create(_aab, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
                            if _aab:FindFirstChild("TextLabel") then
                                _0xH9:Create(_aab.TextLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(200, 200, 200)}):Play()
                            end
                            _aab = nil
                        end
                        if _cpt then pcall(function() _cpt:Stop() end) _cpt = nil end
                        continue
                    end
                    if _ev.source == "AnimTab" then
                        if _aat then _aat:Stop() _aat:Destroy() _aat = nil end
                        local _an = Instance.new("Animation")
                        _an.AnimationId = _ev.id
                        local _tr = _hm:LoadAnimation(_an)
                        if _tr then _tr:Play() _aat = _tr _aai = _ev.id end
                    else
                        if _cpt then pcall(function() _cpt:Stop() end) _cpt = nil end
                        local _tr = _ple(_ev.name, _ev.id)
                        if _tr and typeof(_tr) == "Instance" and _tr:IsA("AnimationTrack") then _cpt = _tr end
                    end
                end
                _cpt = nil
                _aat = nil
                _aai = nil
                if _aab then
                    _0xH9:Create(_aab, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
                    if _aab:FindFirstChild("TextLabel") then
                        _0xH9:Create(_aab.TextLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(200, 200, 200)}):Play()
                    end
                    _aab = nil
                end
            end)
        end)
    end
end
