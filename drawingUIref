local _tgi = {["Wave"]=3576686446,["Sit"]=15571540519,["Point"]=3576823880,["Shrug"]=3576968026,["Royal Drums"]=3360689775,["Clap"]=10370934040,["Beckon"]=5230615437,["Point2"]=6532844183}
local function _scm()
    if not _cme then return end
    _cme = false
    if _clc then _clc:Disconnect() _clc = nil end
    _0xB3:SetEmotesMenuOpen(false)
    if _agui then _agui.Enabled = true end
end
local function _stcm()
    if _cme then return end
    _cme = true
    if _scr then _scr.Enabled = false end
    if _agui then _agui.Enabled = false end
    _0xB3:SetEmotesMenuOpen(true)
    task.spawn(function()
        local _ta = {}
        while _cme do
            local _ch = _0xJ1.LocalPlayer.Character
            local _hm = _ch and _ch:FindFirstChildOfClass("Humanoid")
            if _hm then
                for _, _tr in pairs(_hm:GetPlayingAnimationTracks()) do
                    local _id = _tr.Animation.AnimationId
                    local _cid = tonumber(_id:match("%d+"))
                    local _ist = false
                    if _cid then
                        for _n, _tid in pairs(_tgi) do
                            if _cid == _tid then _ist = true break end
                        end
                    end
                    if not _ta[_tr] and (_ist or not string.find(_id, "507768375")) then
                        _ta[_tr] = true
                        if _cid then
                            for _ot, _ in pairs(_ta) do
                                if _ot ~= _tr and _ot.IsPlaying then _ot:Stop() end
                            end
                            local _sc, _an = pcall(function() return _hm:LoadAnimation(_tr.Animation) end)
                            if _sc and _an then
                                _an:Play(0.1, 1, _tr.Speed)
                                _an.TimePosition = _tr.TimePosition
                                task.spawn(function()
                                    _tr.Stopped:Wait()
                                    _an:Stop()
                                    _an:Destroy()
                                    _ta[_tr] = nil
                                end)
                            end
                        end
                    end
                end
            end
            task.wait(0.01)
        end
    end)
end
local function _hpe(_hm, _nm, _id)
    if _0xF1 then return _hm:PlayEmote(_nm) else return _hm:PlayEmoteAndGetAnimTrackById(_id) end
end
local function _ple(_nm, _id)
    if _isr then
        table.insert(_crt, {id = tonumber(_id) or 0, name = _nm, time = tick() - _rst, source = "UI"})
    end
    pcall(function() if _srb then _srb.Text = "" end end)
    local _ch = _0xJ1.LocalPlayer.Character
    local _hm = _ch and _ch:FindFirstChildOfClass("Humanoid")
    local _ds = _hm and _hm:FindFirstChildOfClass("HumanoidDescription")
    if not _ds then return end
    if _hm.RigType ~= Enum.HumanoidRigType.R6 then
        local _tr
        local _sc = pcall(function() _tr = _hpe(_hm, _nm, _id) end)
        if not _sc then
            _ds:AddEmote(_nm, _id)
            _tr = _hpe(_hm, _nm, _id)
        end
        if _cae and _scr then _scr.Enabled = false end
        return _tr
    end
    return nil
end
local function _ade(_nm, _id, _pr)
    if not (_nm and _id) then return end
    local _od = _ceo[tostring(_id)]
    local _em = {["name"]=_nm,["id"]=_id,["icon"]="rbxthumb://type=Asset&id=".._id.."&w=150&h=150",["price"]=_pr or 0,["index"]=#_emt+1,["sort"]={newestfirst=0,oldestfirst=0,alphabeticfirst=0,alphabeticlast=0,highestprice=0,lowestprice=0},["customOrder"]=_od}
    table.insert(_emt, _em)
end
