local function _isff(...)
    if _0xF1 then return false end
    if isfile then return isfile(...) end
    return false
end
local function _wff(...)
    if _0xF1 then return end
    if writefile then return writefile(...) end
end
local function _rff(...)
    if _0xF1 then return "" end
    if readfile then return readfile(...) end
    return ""
end
local function _snd(_t, _tx)
    if (not _0xF1) and syn and syn.toast_notification then
        syn.toast_notification({Type = "Error", Title = _t, Content = _tx})
    else
        _0xE6:SetCore("SendNotification", {Title = _t, Text = _tx})
    end
end
local function _ste()
    for _i, _em in pairs(_emt) do
        local _eb = _frm:FindFirstChild(tostring(_em.id))
        if not _eb then continue end
        local _if = table.find(_fve, _em.id)
        if _em.customOrder then
            _eb.LayoutOrder = _em.customOrder
        else
            local _sv = (_em.sort and _em.sort[_cst]) or _em.index or 0
            _eb.LayoutOrder = _sv + ((_if and 0) or #_emt)
        end
        local _nl = _eb:FindFirstChild("number")
        if _nl then _nl.Text = tostring((_em.sort and _em.sort[_cst]) or _em.index or "") end
    end
end
local function _ugr()
    local _cw = (0.95 - (_rwc - 1) * 0.005) / _rwc
    _grd.CellSize = UDim2.new(_cw, 0, _cw, 0)
    _grd.CellPadding = UDim2.new(0.005, 0, 0.002, 0)
end
local function _crs(_od, _tx, _sr)
    local _cs = Instance.new("TextButton")
    _cs.SizeConstraint = Enum.SizeConstraint.RelativeXX
    _cs.Size = UDim2.new(1, 0, 0.2, 0)
    _cs.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    _cs.LayoutOrder = _od
    _cs.TextColor3 = Color3.new(1, 1, 1)
    _cs.Text = _tx
    _cs.TextScaled = true
    _cs.BorderSizePixel = 0
    _cs.Font = Enum.Font.GothamBold
    Instance.new("UICorner").Parent = _cs
    _cs.Parent = _sfr
    _cs.MouseButton1Click:Connect(function()
        _sfr.Visible = false
        _cst = _sr
        _ste()
    end)
    return _cs
end
