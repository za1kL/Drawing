local function _sao()
    local _od = {}
    table.sort(_aga, function(_a, _b) return _a.order < _b.order end)
    for _i, _dt in ipairs(_aga) do table.insert(_od, {n = _dt.n, o = _dt.order}) end
    _wff("animationplacementsbyza1k.txt", _0xI0:JSONEncode(_od))
end
local function _lao()
    if _isff("animationplacementsbyza1k.txt") then
        local _sc, _rs = pcall(function() return _0xI0:JSONDecode(_rff("animationplacementsbyza1k.txt")) end)
        if _sc and _rs then
            for _, _sv in ipairs(_rs) do
                for _, _dt in ipairs(_aga) do if _dt.n == _sv.n then _dt.order = _sv.o break end end
            end
        end
    end
end
local function _gsa()
    local _st = {}
    for _, _dt in ipairs(_aga) do table.insert(_st, _dt) end
    table.sort(_st, function(_a, _b) return _a.order < _b.order end)
    return _st
end
local function _uab()
    local _st = _gsa()
    for _ix, _dt in ipairs(_st) do
        for _, _bn in ipairs(_acn:GetChildren()) do
            if _bn:IsA("TextButton") and _bn.Name == _dt.n then
                local _lb = _bn:FindFirstChild("TextLabel")
                if _lb then _lb.Text = _sna and (_dt.n .. " #" .. _dt.order) or _dt.n end
                _bn.LayoutOrder = _dt.order
                break
            end
        end
    end
end
local function _cri(_dt)
    local _it = Instance.new("Frame")
    _it.Size = UDim2.new(1, 0, 0, 35)
    _it.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    _it.BorderSizePixel = 0
    _it.LayoutOrder = _dt.order
    _it.Parent = _rsc
    local _is = Instance.new("UIStroke")
    _is.Color = Color3.fromRGB(35, 35, 35)
    _is.Thickness = 1
    _is.Parent = _it
    local _nl = Instance.new("TextLabel")
    _nl.Size = UDim2.new(1, -80, 1, 0)
    _nl.Position = UDim2.new(0, 10, 0, 0)
    _nl.BackgroundTransparency = 1
    _nl.Text = _dt.n
    _nl.Font = Enum.Font.Gotham
    _nl.TextColor3 = Color3.fromRGB(200, 200, 200)
    _nl.TextSize = 13
    _nl.TextXAlignment = Enum.TextXAlignment.Left
    _nl.Parent = _it
    local _nb = Instance.new("TextBox")
    _nb.Size = UDim2.new(0, 50, 0, 25)
    _nb.Position = UDim2.new(1, -60, 0.5, -12)
    _nb.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    _nb.BorderSizePixel = 0
    _nb.Text = tostring(_dt.order)
    _nb.Font = Enum.Font.Gotham
    _nb.TextColor3 = Color3.fromRGB(200, 200, 200)
    _nb.TextSize = 13
    _nb.ClearTextOnFocus = false
    _nb.Parent = _it
    _nb.FocusLost:Connect(function()
        local _nm = tonumber(_nb.Text)
        if _nm and _nm >= 1 and _nm <= #_aga and _nm == math.floor(_nm) then
            local _oo = _dt.order
            for _, _od in ipairs(_aga) do if _od.order == _nm and _od ~= _dt then _od.order = _oo break end end
            _dt.order = _nm
            for _, _oi in ipairs(_rsc:GetChildren()) do if _oi:IsA("Frame") then _oi:Destroy() end end
            local _st = _gsa()
            for _, _ad in ipairs(_st) do _cri(_ad) end
            _uab()
            _sao()
        else _nb.Text = tostring(_dt.order) end
    end)
end
local function _tge(_id, _nm, _bn)
    local _ch = _0xJ1.LocalPlayer.Character
    local _hm = _ch and _ch:FindFirstChildOfClass("Humanoid")
    if not _hm then return end
    if _aai == _id then
        if _aat then _aat:Stop() _aat:Destroy() _aat = nil end
        _aai = nil
        if _isr then table.insert(_crt, {id = "STOP", name = "Stop", time = tick() - _rst, source = "AnimTab"}) end
        if _aab then
            _0xH9:Create(_aab, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
            if _aab:FindFirstChild("TextLabel") then _0xH9:Create(_aab.TextLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(200, 200, 200)}):Play() end
            _aab = nil
        end
        return
    end
    if _aat then
        if _isr then table.insert(_crt, {id = "STOP", name = "Stop", time = tick() - _rst, source = "AnimTab"}) end
        _aat:Stop()
        _aat:Destroy()
        _aat = nil
    end
    if _aab then
        _0xH9:Create(_aab, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
        if _aab:FindFirstChild("TextLabel") then _0xH9:Create(_aab.TextLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(200, 200, 200)}):Play() end
    end
    if _isr then table.insert(_crt, {id = _id, name = _nm, time = tick() - _rst, source = "AnimTab"}) end
    local _an = Instance.new("Animation")
    _an.AnimationId = _id
    local _tr = _hm:LoadAnimation(_an)
    if _tr then
        _tr:Play()
        _aat = _tr
        _aai = _id
        _aab = _bn
        _0xH9:Create(_bn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
        if _bn:FindFirstChild("TextLabel") then _0xH9:Create(_bn.TextLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(0, 0, 0)}):Play() end
        task.spawn(function()
            _tr.Stopped:Wait()
            if _aai == _id then
                _aai = nil
                _aat = nil
                _0xH9:Create(_bn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
                if _bn:FindFirstChild("TextLabel") then _0xH9:Create(_bn.TextLabel, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(200, 200, 200)}):Play() end
                _aab = nil
            end
        end)
    end
end
